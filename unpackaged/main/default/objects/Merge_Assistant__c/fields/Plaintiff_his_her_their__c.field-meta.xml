<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Plaintiff_his_her_their__c</fullName>
    <externalId>false</externalId>
    <formula>If(
				(Number_of_Plaintiffs__c &gt; 1), &quot;their&quot;,
					IF(
		    ISPICKVAL(Plaintiff_Gender__c,&quot;Male&quot;),&quot;his&quot;,
				    IF(
								 ISPICKVAL(Plaintiff_Gender__c,&quot;Female&quot;),&quot;her&quot;,
												NULL)))</formula>
    <label>Plaintiff his/her/their</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
