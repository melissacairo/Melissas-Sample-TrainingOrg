<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>litify_pm__Label_And_AnswerType_Not_Null</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
            OR(ISBLANK(litify_pm__Question_Label__c),ISNULL(litify_pm__Question_Label__c)),
            ISBLANK(TEXT(litify_pm__Answer_Type__c)))</errorConditionFormula>
    <errorMessage>Question Label or Answer Type cannot be null</errorMessage>
</ValidationRule>
